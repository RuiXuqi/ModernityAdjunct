{
    "multipart": [
        {   "when": { "dir": "x" },
            "apply": { "model": "tofucraft:chikuwaPlatform_inner"}
        },
        {   "when": { "dir": "x", "con": "single" },
            "apply": { "model": "tofucraft:chikuwaPlatformPlain_single"}
        },
        {   "when": { "dir": "x", "con": "left" },
            "apply": { "model": "tofucraft:chikuwaPlatformPlain_left"}
        },
        {   "when": { "dir": "x", "con": "middle" },
            "apply": { "model": "tofucraft:chikuwaPlatformPlain_middle"}
        },
        {   "when": { "dir": "x", "con": "right" },
            "apply": { "model": "tofucraft:chikuwaPlatformPlain_right" }
        },
        {   "when": { "dir": "z" },
            "apply": { "model": "tofucraft:chikuwaPlatform_inner", "y": 90 }
        },
        {   "when": { "dir": "z", "con": "single" },
            "apply": { "model": "tofucraft:chikuwaPlatformPlain_single", "y": 90 }
        },
        {   "when": { "dir": "z", "con": "left" },
            "apply": { "model": "tofucraft:chikuwaPlatformPlain_left", "y": 90 }
        },
        {   "when": { "dir": "z", "con": "middle" },
            "apply": { "model": "tofucraft:chikuwaPlatformPlain_middle", "y": 90 }
        },
        {   "when": { "dir": "z", "con": "right" },
            "apply": { "model": "tofucraft:chikuwaPlatformPlain_right", "y": 90 }
        }
    ]
}